{
  "name": "next-lite-framework",
  "version": "0.1.0",
  "description": "A lightweight, high-performance alternative to Next.js built with esbuild. Features file-based routing, HMR, CSS Modules, and TypeScript support out of the box, all while maintaining a minimal footprint and blazing-fast build times.",
  "main": "dist/index.js",
  "bin": {
    "create-next-lite-app": "./scripts/create-app.js"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "start": "node scripts/prod-server.js",
    "clean": "rd /s /q .next dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "nextjs",
    "framework",
    "typescript",
    "esbuild",
    "fast",
    "lightweight"
  ],
  "author": "Teck",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/teckmill/next-lite-main"
  },
  "bugs": {
    "url": "https://github.com/teckmill/next-lite-main/issues"
  },
  "homepage": "https://github.com/teckmill/next-lite-main#readme",
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "dotenv": "^16.5.0",
    "esbuild": "^0.19.12",
    "fs-extra": "^11.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.49",
    "postcss-modules": "^6.0.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
