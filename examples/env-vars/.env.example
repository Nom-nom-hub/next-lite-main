# Next-Lite Environment Variables Example
# Copy this file to .env to use these variables

# Server configuration
PORT=3000
NEXT_LITE_SSR=false

# Application settings
NEXT_LITE_APP_NAME="Environment Variables Example"
NEXT_LITE_API_URL="https://api.example.com"
NEXT_LITE_DEBUG=true

# Feature flags
NEXT_LITE_ENABLE_FEATURE_A=true
NEXT_LITE_ENABLE_FEATURE_B=false
